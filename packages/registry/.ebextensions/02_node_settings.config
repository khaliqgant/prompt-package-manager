# Node.js application settings
# Note: Modern Node.js platforms don't support legacy options like
# NodeVersion, GzipCompression, ProxyServer - these are now managed by the platform
# Gzip is handled in nginx config (05_nginx.config)
# Start command is in Procfile

option_settings:
  # Application environment variables
  aws:elasticbeanstalk:application:environment:
    NODE_ENV: "production"
    NPM_USE_PRODUCTION: "true"
    NPM_CONFIG_PRODUCTION: "true"
